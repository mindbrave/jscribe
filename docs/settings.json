{
    "INPUT_PATHS": ["./"],
    "IGNORE_PATHS_REGEX": ["./tests.*?$", "./templates.*?$"],
    "FILE_REGEX": ".*?[.]py$",
    "FILE_IGNORE_REGEX": null,
    "DOCUMENTATION_OUTPUT_PATH": "./docs/",
    "DOC_STRING_REGEX": ["[\"][\"][\"][*]", "[\"][\"][\"]"],
    "DOC_STRING_LINE_PREFIX": "",
    "TAG_REGEX": "[@](?P<tag>.*?)\\s",
    "IGNORE_INVALID_TAGS": false,
    "TEMPLATE": "default",
    "TEMPLATE_SETTINGS": {
        "SHOW_LINE_NUMBER": true,
        "FOOTER_TEXT": "JSCRIBE",
        "LOGO_PATH": "",
        "ELEMENT_TEMPLATES": {
            "class": "class_element.html"
        }
    },
    "TAG_SETTINGS": "jscribe.conf.pythontagsettings",
    "LANGUAGE": "python",
    "ALL_SOURCE_FILES": true
}

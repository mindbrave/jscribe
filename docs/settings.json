{
    "INPUT_PATHS": ["./"],
    "IGNORE_PATHS_REGEX": ["./tests.*?$", "./templates.*?$"],
    "FILE_REGEX": ".*?[.]py$",
    "FILE_IGNORE_REGEX": null,
    "DOCUMENTATION_OUTPUT_PATH": "./docs/",
    "DOC_STRING_REGEX": ["[\"][\"][\"][*]", "[\"][\"][\"]"],
    "TAG_REGEX": "[@](?P<tag>.*?)\\s",
    "IGNORE_INVALID_TAGS": false,
    "TEMPLATE": "default",
    "TEMPLATE_SETTINGS": {
        "SHOW_LINE_NUMBER": true,
        "FOOTER_TEXT": "JSCRIBE v0.2dev",
        "TITLE": "JScribe documentation",
        "ELEMENT_TEMPLATES": {}
    },
    "TAG_SETTINGS": "jscribe.conf.pythontagsettings",
    "LANGUAGE": "python",
    "ALL_SOURCE_FILES": false
}
